{
  "name": "console-plugin-template",
  "version": "0.0.1",
  "displayName": "OpenShift Console Pendo Plugin",
  "description": "Runs pendo on the console.",
  "dependencies": {
    "@console/pluginAPI": "*"
  },
  "extensions": [
    {
      "type": "console.telemetry/listener",
      "properties": {
        "listener": {
          "$codeRef": "pendo"
        }
      }
    }
  ]
}